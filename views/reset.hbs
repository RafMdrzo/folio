<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <!-- SCRIPTS -->
  <script async src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>  
  <script async src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
  <script async src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
  <script defer src="js/typed.js"></script>
  <script defer src="js/folio.js"></script>

  <!-- BOOTSTRAP AND CSS -->
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
  <link rel="stylesheet" href="css/login.css" type="text/css">

  <!-- TITLE -->
  <title>
    f o l i o .
  </title>
</head>
<body>
  <!-- TYPED.JS -->
  <div class="vertical-center">
    <div id="loginContainer" class="container" align="center">
      <div class="container" align="center" id="folio">
        <h1><span id="typed"></span></h1>
        <script>
        $('document').ready(function(){
          var typed = new Typed('#typed', {
            strings:["f o l i o ."],
            backSpeed: 40,
            typeSpeed: 55,
            onComplete: function () {
              $(".typed-cursor").hide();
            }
          });
        });

        </script>
      </div>

      <!-- IMAGE SWAP -->
      <script src="js/imgswap.js"></script>

      <!--RESET -->
      <div class="container" id="resetForm" align = "center">
        <form id="resetForm" method="POST" action="home">
          <div class="form-group" align="left">
            <h6 for="uName" ></h6>
            <input type="text" class="form-control form-control-lg" id="#pw_" placeholder="new password" name="newPass">
          </div>

          <div class="form-group" align="left" id="passEdit">
            <h6 for="pWord"></h6>
            <input type="password" class="form-control form-control-lg" id="#cp_w" placeholder="confirm password" name="confPass">
          </div>
            <input type="hidden" name="formInstance" value="form1">

          <div class="container" id="buttonCont" align="center">
            <button id="submitBtn" type="submit" class="btn-lg" align="center">submit</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</body>
</html>
